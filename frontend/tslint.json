{
  "defaultSeverity": "error",
  "extends": ["tslint-react", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "member-access": false,
    "ordered-imports": false,
    "quotemark": false,
    "no-console": false,
    "semicolon": false,
    "jsx-no-lambda": false,
    "prettier": true
  },
  "rulesDirectory": ["tslint-plugin-prettier"],
  "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*.ts"]
  }
}
